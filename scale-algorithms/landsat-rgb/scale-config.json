{
  "import": {
    "job_types": [{
      "name": "landsat-rgb",
      "version": "1.0.0",
      "title": "Landsat RGB",
      "description": "Combines Landsat bands 4 (R), 3 (G), and 2 (B) into single GeoTIFF.",
      "category": "Landsat",
      "author_name": "Eric Svendsen, Applied Information Sciences",
      "author_url": "http://www.appliedis.com",
      "is_operational": true,
      "icon_code": "f03e",
      "docker_privileged": false,
      "docker_image": "10.4.4.10:5000/landsat-rgb:dev",
      "priority": 1,
      "timeout": 3600,
      "max_tries": 3,
      "cpus_required": 1.0,
      "mem_required": 512.0,
      "disk_out_const_required": 2048.0,
      "disk_out_mult_required": 0.0,
      "interface": {
        "output_data": [{
          "media_type": "image/tiff",
          "required": true,
          "type": "file",
          "name": "landsat-rgb"
        }],
        "shared_resources": [],
        "command_arguments": "-r ${red} -g ${green} -b ${blue}",
        "input_data": [{
          "media_types": ["image/tiff"],
          "required": true,
          "type": "file",
          "name": "red"
        },{
          "media_types": ["image/tiff"],
          "required": true,
          "type": "file",
          "name": "green"
        },{
          "media_types": ["image/tiff"],
          "required": true,
          "type": "file",
          "name": "blue"
        }],
        "version": "1.0",
        "command": "landsat-rgb.sh"
      }
    }]
  }
}